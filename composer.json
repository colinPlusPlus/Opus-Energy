{
  "name": "colinplusplus/Opus-Energy",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "Custom wordpress theme for Opus-Energy",
  "homepage": "http://mainstreetcreativeco.com",
  "authors": [
    {
      "name": "Colin Williams",
      "email": "colin@mainstreetcreativeco.com",
      "homepage": "https://github.com/colinplusplus"
    },
    {
      "name": "Stacy McCrady",
      "email": "staceymccrady@hotmail.com",
      "homepage": "https://github.com/babylonn"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/roots/sage/issues",
    "forum": "https://discourse.roots.io/"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "~1.0",
    "wpackagist-plugin/advanced-custom-fields": "*",
    "wpackagist-plugin/akismet": "*",
    "wpackagist-plugin/jetpack": "*",
    "wpackagist-plugin/members": "1.1.1",
    "wpackagist-plugin/redirection": "*",
    "wpackagist-plugin/regenerate-thumbnails": "*",
    "wpackagist-plugin/search-and-replace": "*",
    "wpackagist-plugin/simple-social-icons": "*",
    "wpackagist-plugin/uk-cookie-consent": "*",
    "wpackagist-plugin/wp-pagenavi": "*",
    "wpackagist-plugin/siteorigin-panels": "2.2.2",
    "wpackagist-plugin/so-css": "1.0.4",
    "roots/soil": "3.6.2",
    "wpackagist-plugin/custom-options-plus": "1.5",
    "wpackagist-plugin/so-widgets-bundle": "1.5.4"
  },
  "extra": {
    "installer-paths": {
      "../../mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "../../plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}